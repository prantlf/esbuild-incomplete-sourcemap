{
  "name": "esbuild-incomplete-sourcemap",
  "version": "0.0.1",
  "description": "Demonstrates missing sources in the bundle source map generated by esbuild, if the sources were processed by a plugin.",
  "homepage": "https://github.com/prantlf/esbuild-incomplete-sourcemap#readme",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/esbuild-incomplete-sourcemap.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/esbuild-incomplete-sourcemap/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/esbuild-incomplete-sourcemap/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "npm run build:plugin && npm run build:example",
    "build:plugin": "node build/plugin",
    "build:example": "node build/example.esbuild",
    "build:example:rollup": "rollup -c build/example.rollup.js",
    "start": "python -m SimpleHTTPServer"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "esbuild": "^0.14.8",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "rollup": "^2.62.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "typescript": "^4.5.4"
  }
}
